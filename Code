local function clip()
	local function run()
		local UIS = game:GetService('UserInputService')

		UIS.InputBegan:Connect(function(inp)
			if (inp.KeyCode == Enum.KeyCode.LeftControl and UIS:IsKeyDown(Enum.KeyCode.G)) or (inp.KeyCode == Enum.KeyCode.G and UIS:IsKeyDown(Enum.KeyCode.LeftControl)) then
				local h = game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart')
				h.Size = h.Size + Vector3.new(2,0,0)
				local par = RaycastParams.new()
				par.FilterDescendantsInstances = {h.Parent}
				local ray = workspace:Raycast((h.CFrame*CFrame.new(0,0,-100)).p,(h.CFrame*CFrame.Angles(0,math.rad(180),0)).LookVector*120,par)
				if ray and table.find(h:GetTouchingParts(),ray.Instance) then
					h.CFrame = CFrame.lookAt(ray.Position,h.Position)
				end
				h.Size = h.Size - Vector3.new(2,0,0)
			end
		end)
	end
	local info = {
		run = run,
		description = [[On 12/16/2021, ROBLOX removed the ability to go to a corner and clip to the other side, no longer!
Simply walk up to a wall (doesn't have to be the corners), and click CTRL + G.
Note that this script is not completely accurate and you may have to adjust your character if it doesn't work, and this also has support for R15.]],
		name = 'Bring Back Clipping',
		icon = 'rbxassetid://8268082307',
		owner = 'Brisk'
	}

	return info
end

local function fly()
	local function run()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Scriptonix-YOUTUBE/Fly/main/code",true))()
	end
	local info = {
		run = run,
		description = [[Not your regular fly, this can work on almost every game!
Click Ctrl-E to start/stop flying.]],
		name = 'FE Fly',
		icon = 'rbxassetid://8638542565',
		owner = 'Brisk'
	}

	return info
end

local myscripts = {
	chatgui(),
	extrakeys(),
	skipfrappe(),
	searchfling(),
	removeface(),
	invisicam(),
	freecam(),
	krnlui(),
	zoom(),
	boombox(),
	emotes(),
	menu(),
	hidden(),
	staircase(),
	clip(),
	fly()
}

local otherscripts = {
	ball()
}


local function change(inf)
	sc = inf
	creatorname.Text = inf.owner
	scname.Text = inf.name
	scname_2.Text = inf.description
	icon_2.Image = inf.icon
	foreshadow.Visible = true
	if table.find(file,sc.name) then
		save.Text = 'Unsave'
	else
		save.Text = 'Save'
	end
end

local function loadscripts(tab)
	for _, v in pairs(ScrollingFrame:GetChildren()) do
		if v.Name == 'del' then
			v:Destroy()
		end
	end
	for _, v in pairs(tab) do
		local clone = scriptexample:Clone()
		clone.Visible = true
		clone.Name = 'del'
		clone.name.Text = v.name
		clone.desc.Text = v.description
		clone.icon.Image = v.icon
		clone.Parent = ScrollingFrame
		clone.MouseButton1Click:Connect(function()
			if foreshadow.Visible == false then
				change(v)
			end
		end)
	end
end

local function runsaves()
	for _, v in pairs(file) do
		for _, scri in pairs(myscripts) do
			if v == scri.name then
				scri.run()
			end
		end
		for _, scri in pairs(otherscripts) do
			if v == scri.name then
				scri.run()
			end
		end
	end
end

local function bringup()
	local tween = TS:Create(main,TweenInfo.new(2),{Position = UDim2.new(.5,0,.15,0)})
	tween:Play()
end

local function bringdown()
	local tween = TS:Create(main,TweenInfo.new(2),{Position = UDim2.new(.5,0,1,0)})
	tween:Play()
end


if _G.EnableToggleButton then
	closeitall.Name = "closeitall"
	closeitall.Parent = main
	closeitall.BackgroundColor3 = Color3.fromRGB(32, 32, 35)
	closeitall.Position = UDim2.new(0.360995859, 0, -0.0589569174, 0)
	closeitall.Size = UDim2.new(0, 200, 0, 47)
	closeitall.AutoButtonColor = false
	closeitall.Font = Enum.Font.SourceSans
	closeitall.Text = " "
	closeitall.TextColor3 = Color3.fromRGB(0, 0, 0)
	closeitall.TextSize = 14.000

	noer.Name = "noer"
	noer.Parent = closeitall

	imag.Name = "imag"
	imag.Parent = closeitall
	imag.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	imag.BackgroundTransparency = 1.000
	imag.Position = UDim2.new(0.430152297, 0, 0.0638297871, 0)
	imag.Size = UDim2.new(0, 26, 0, 23)
	imag.Image = "rbxassetid://1312007357"
	imag.ImageColor3 = Color3.fromRGB(90, 90, 90)

	closeitall.MouseButton1Click:Connect(function()
		if t and w then
			spawn(function() w = false wait(3) w = true end)
			t = false
			bringup()
		elseif not t and w then
			spawn(function() w = false wait(3) w = true end)
			t = true
			bringdown()
		end
	end)
else
	closeitall:Destroy()
	noer:Destroy()
	imag:Destroy()
end


UIS.InputBegan:Connect(function(imn)
	if (imn.KeyCode == Enum.KeyCode.LeftControl and UIS:IsKeyDown(Enum.KeyCode.M)) or (imn.KeyCode == Enum.KeyCode.M and UIS:IsKeyDown(Enum.KeyCode.LeftControl)) then
		if t and w then
			spawn(function() w = false wait(3) w = true end)
			t = false
			bringup()
		elseif not t and w then
			spawn(function() w = false wait(3) w = true end)
			t = true
			bringdown()
		end
	end
end)

better.MouseButton1Click:Connect(function()
	ty = myscripts
	loadscripts(myscripts)
end)

other.MouseButton1Click:Connect(function()
	ty = otherscripts
	loadscripts(otherscripts)
end)

exec.MouseButton1Click:Connect(function()
	sc.run()
end)

save.MouseButton1Click:Connect(function()
	if save.Text == 'Save' then
		table.insert(file,sc.name)
		save.Text = 'Unsave'
		writefile('ScriptHubSaves.txt',http:JSONEncode(file))
	else
		table.remove(file,table.find(file,sc.name))
		save.Text = 'Save'
		writefile('ScriptHubSaves.txt',http:JSONEncode(file))
	end
end)

en.MouseButton1Click:Connect(function()
	foreshadow.Visible = false
end)

TextBox.FocusLost:Connect(function()
	local new = {}
	for _, v in pairs(ty) do
		if string.find(string.lower(v.name),string.lower(TextBox.Text)) then
			table.insert(new,v)
		end
	end
	loadscripts(new)
end)

ty = myscripts
loadscripts(myscripts)
runsaves()
