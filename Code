local function zoom()
	local function run()
		local LocalPlayer = game.Players.LocalPlayer

		LocalPlayer.CameraMaxZoomDistance = 99999999 
		LocalPlayer.CameraMinZoomDistance = -99999999

		LocalPlayer.Changed:Connect(function()
			LocalPlayer.CameraMaxZoomDistance = 99999999 
			LocalPlayer.CameraMinZoomDistance = -99999999
		end)
	end
	local info = {
		run = run,
		description = [[Removes your zoom limit, allowing you to zoom out almost infinitely.]],
		name = 'Remove Zoom Limit',
		icon = 'rbxassetid://82105965',
		owner = 'Brisk'
	}

	return info
end

local function boombox()
	local function run()
		local function createbutton(g)
			local b = Instance.new('TextButton',g)
			b.Size = UDim2.new(.044,0,.101,0)
			b.Position = UDim2.new(.955,0,.001,0)
			b.Text = 'X'
			b.TextColor3 = Color3.fromRGB(255,255,255)
			b.Font = Enum.Font.SourceSansBold
			b.TextScaled = true
			b.BackgroundTransparency = 1
			b.MouseButton1Click:Wait()
			g.Parent:Destroy()
		end

		repeat game:GetService("RunService").RenderStepped:wait() until game.Players.LocalPlayer ~= nil

		game.Players.LocalPlayer:WaitForChild('PlayerGui').ChildAdded:Connect(function(g)
			if g.Name == 'ChooseSongGui' then
				createbutton(g:WaitForChild'Frame')
			end
		end)
	end
	local info = {
		run = run,
		description = [[Gives you an 'X' button to Boombox gears, which allows you to cancel.]],
		name = 'Boombox Cancel Button',
		icon = 'rbxassetid://8072628868',
		owner = 'Brisk'
	}

	return info
end

