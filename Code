local file = nil

local y,re = pcall(function()
	file = readfile('ScriptHubSaves.txt')
end)

if not y then
	writefile('ScriptHubSaves.txt','[]')
	file = http:JSONDecode(readfile('ScriptHubSaves.txt'))
else
	file = http:JSONDecode(file)
end

local function chatgui()
	local function run()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Scriptonix-YOUTUBE/Roblox-Chat/main/Code",true))()
	end
	local info = {
		run = run,
		description = [[Loads a nifty little GUI you can use to troll people.]],
		name = 'Chat GUI',
		icon = 'rbxassetid://8057956608',
		owner = 'Brisk'
	}

	return info
end

local function extrakeys()
	local function run()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/Scriptonix-YOUTUBE/Roblox-Keys/main/Code",true))()
	end
	local info = {
		run = run,
		description = [[This gives you the following keybinds:
		-(F1): Locks camera to head (you can get some pretty sick screenshots with it)
		-(F2): Locks camera to torso (you can get some pretty sick screenshots with it)
		-(<): Aligns camera rotation to the left (useful for wall hopping in some games)
		-(>): Aligns camera rotation to the right (useful for wall hopping in some games)
		-(ctrl + b): Opens emote menu (changes default > keybind so that alignment keys was possible)]],
		name = 'Extra Keys',
		icon = 'rbxassetid://272678320',
		owner = 'Brisk'
	}

	return info
end

local function skipfrappe()
	local function run()
		if game.PlaceId == 5580097107 then
			local d = game.Players.LocalPlayer.PlayerGui:WaitForChild'Gui':WaitForChild'OOBE'
			d.Changed:Connect(function()
				if d.Visible then
					game.ReplicatedStorage.Resources.RemoteFunction:InvokeServer("setOOBEStatus",false)
					local ts = game:GetService("TeleportService")local p = game:GetService("Players").LocalPlayer ts:Teleport(game.PlaceId, p)
				end
			end)
		end
	end
	local info = {
		run = run,
		description = [[Skips the annoying (literally impossible to beat without rejoining) rules segment Frappe won't fix.]],
		name = 'Skip Frappe Rules Segment',
		icon = 'rbxassetid://839578807',
		owner = 'Brisk'
	}

	return info
end

local function searchfling()
	local function run()
		_G.DisplaySkids = true
		_G.AntiFling = true
		_G.LoopSeconds = 60
		_G.LoopSearch = false

		loadstring(game:HttpGet("https://raw.githubusercontent.com/Scriptonix-YOUTUBE/Search/main/Code",true))()
	end
	local info = {
		run = run,
		description = [[Searches the server for skids using InvisFling,
		and prints out the result in the developer console (F9). This also provides immunity from the skid.
		]],
		name = 'Invis Fling Searcher',
		icon = 'rbxassetid://8066010277',
		owner = 'Brisk'
	}

	return info
end

local function removeface()
	local function run()
		local function doe(c)
			c.Archivable = true

			local h = c:WaitForChild'Head'
			local f = h:WaitForChild('face',true)

			if f then f:Destroy() end
		end
		local l = game.Players.LocalPlayer
		local char = l.Character or l.CharacterAdded:Wait()
		doe(char)
		l.CharacterAdded:Connect(function(c)
			doe(c)
		end)
	end
	local info = {
		run = run,
		description = [[Removes your face decal until you rejoin.]],
		name = 'Remove Face',
		icon = 'rbxassetid://8066162469',
		owner = 'Brisk'
	}

	return info
end

local function invisicam()
	local function run()
		game:GetService'Players'.LocalPlayer.DevCameraOcclusionMode = 'Invisicam'
		game:GetService'Players'.LocalPlayer.Changed:Connect(function()
			game:GetService'Players'.LocalPlayer.DevCameraOcclusionMode = 'Invisicam'
		end)
	end
	local info = {
		run = run,
		description = [[Allows you to put your camera through things and not be bound to the annoying objects in your view.]],
		name = 'Invisicam',
		icon = 'rbxassetid://8071864794',
		owner = 'Brisk'
	}

	return info
end

